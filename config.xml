<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.teleschool_digital_marketing" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Digital Marketing</name>
  <description>
      A digital Marketing E-learning course developed and distributed by TeleSchool Zimbabwe.
  </description>
  <author email="fgmavaza@gmail.com" href="https://teleschool.co.zw">
     Teleschool Geeks
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
<!--
        ldpi    : 36x36 px
        mdpi    : 48x48 px
        hdpi    : 72x72 px
        xhdpi   : 96x96 px
        xxhdpi  : 144x144 px
        xxxhdpi : 192x192 px
    -->
    <icon src="res/ldpi.png" density="ldpi" />
    <icon src="res/mdpi.png" density="mdpi" />
    <icon src="res/hdpi.png" density="hdpi" />
    <icon src="res/xhdpi.png" density="xhdpi" />
    <icon src="res/xxhdpi.png" density="xxhdpi" />
    <icon src="res/xxxhdpi.png" density="xxxhdpi" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />

  </platform>
</widget>